<div class="booyah-box col-10 offset-1">
    <h1>Edit Your Place </h1><br />
    <%= simple_form_for @place do |f| %>
    <%= f.input :name, label: "Name" %>
    <%= f.input :address, label: "Address change?" %>
    <%= f.input :description, label: "Please describe your experience" %>
    <br />
    <%= f.submit 'Update', class: 'btn btn-primary' %>
    <% end %>
</div>